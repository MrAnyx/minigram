{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": ["plugin:vue/vue3-essential", "airbnb-base", "prettier"],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "globals": {
      "chrome": false
   },
   "plugins": ["vue", "prettier"],
   "rules": {
      "prettier/prettier": "error",
      "import/prefer-default-export": "off",
      "no-console": "off",
      "import/no-extraneous-dependencies": "off",
      "no-unused-vars": "off",
      "vue/valid-v-slot": "off"
   }
}
